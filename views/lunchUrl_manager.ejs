<!-- Content Header (Page header) -->
<section class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1></h1>
            </div>
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                    <li class="breadcrumb-item active">url-manager</li>
                </ol>
            </div>
        </div>
    </div><!-- /.container-fluid -->
</section>

<!-- Main content -->
<section class="content">
    <div class="row">
        <div class="col-12">
            <!-- general form elements -->
            <div class="card card-primary">
                <div class="card-header">
                    <h3 class="card-title">Add link</h3>
                </div>
                <!-- /.card-header -->
                <!-- form start -->
                <form id="frmPost">
                    <div class="card-body">
                        <div class="form-group">
                            <label for="exampleInputEmail1">Youtube</label>
                            <input type="text" class="form-control" id="url_youtube" name="url_youtube"
                                placeholder="Youtube url">
                        </div>
                        <div class="form-group">
                            <label for="exampleInputEmail1">Bulb</label>
                            <input type="text" class="form-control" id="url_bulb" name="url_bulb"
                                placeholder="Bulb url">
                        </div>
                        <div class="form-group">
                            <label for="exampleInputEmail1">Book</label>
                            <input type="text" class="form-control" id="url_book" name="url_book"
                                placeholder="Book url">
                        </div>
                        <div class="form-group">
                            <label for="exampleInputEmail1">Burger menu</label>
                            <input type="text" class="form-control" id="url_burgemenu" name="url_burgemenu"
                                placeholder="Burger url">
                        </div>
                        <div class="form-group">
                            <label for="exampleInputEmail1">Ear</label>
                            <input type="text" class="form-control" id="url_ear" name="url_ear" placeholder="Ear url">
                        </div>
                    </div>
                    <!-- /.card-body -->

                    <div class="card-footer">
                        <button type="submit" class="btn btn-primary">Submit</button>
                    </div>
                </form>
            </div>
            <!-- /.card -->
            <!-- /.card -->
        </div>
        <!-- /.col -->
    </div>
    <!-- /.row -->
</section>
<!-- /.content -->
</div>

<%- contentFor('myjavascript')%>

<script src="../public/myjavascript/lunchUrl.js"></script>